            <div class="details">
                <div class="recentOrders">
                    <div class="cardHeader">
                        <h2>Recent Activity</h2>
                        <a href="#" class="btn">View All</a>
                    </div>

                    <table>
                        <thead>
                            <tr>
                                <td>Name</td>
                                <td>Followers</td>
                                <td>Post</td>
                                <td>Following</td>
                            </tr>
                        </thead>

                        <tbody>
                            <tr>
                                <td>January</td>
